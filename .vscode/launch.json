	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "debug",
			"cargo": {
				"args": [
					"build",
					"--bin=adventofcode_rust",
					"--package=adventofcode_rust",
					"--release",
				],
				"filter": {
					"name": "adventofcode_rust",
